        android:textSize="@dimen/_20dp"
        android:fontFamily="@font/josefin_sans_bold"
        android:textColor="@color/birutua"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView24" />

    <TextView
        android:id="@+id/textView97"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_10sdp"
        android:layout_marginTop="@dimen/_20sdp"
        android:layout_marginEnd="@dimen/_20sdp"
        android:text="We can help you or answering your question.
Contact us at :"
        android:textColor="@color/abu"
        android:textSize="@dimen/_14dp"
        android:fontFamily="@font/josefin_sans_bold"
        android:textStyle="normal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView96" />

    <TextView
        android:id="@+id/textView98"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_10sdp"
        android:layout_marginTop="@dimen/_20sdp"
        android:layout_marginEnd="@dimen/_20sdp"
        android:text="    Kliringid@mail.com"
        android:fontFamily="@font/josefin_sans_bold"
        android:textStyle="normal"
        android:textSize="@dimen/_16dp"
        android:textColor="@color/birutua"
        android:drawableStart="@drawable/vektoremail"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView97" />

    <TextView
        android:id="@+id/textView99"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_10sdp"
        android:layout_marginTop="@dimen/_20sdp"
        android:layout_marginEnd="@dimen/_20sdp"
        android:text="     086799052022"
        android:textColor="@color/birutua"
        android:textSize="@dimen/_16dp"
        android:textStyle="normal"
        android:drawableStart="@drawable/vektortrlpon"
        android:fontFamily="@font/josefin_sans_bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView98" />
</androidx.constraintlayout.widget.ConstraintLayout>